[push]
	default = tracking
[color]
	diff = auto
	status = auto
	branch = auto
	grep = auto
	interactive = auto
	ui = auto
	pager = true
[apply]
	whitespace=fix
[diff]
	algorithm = patience
[alias]
    ls-subtrees = !"git log | grep git-subtree-dir | tr -d ' ' | cut -d ":" -f2 | sort | uniq | xargs -I {} bash -c 'if [ -d $(git rev-parse --show-toplevel)/{} ] ; then echo {}; fi'"

