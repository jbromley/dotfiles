!! X Resource File

! General color settings

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6
! black
*.color0:       #1d1f21
*.color8:       #969896
! red
*.color1:       #cc6666
*.color9:       #cc6666
! green
*.color2:       #b5bd68
*.color10:      #b5bd68
! yellow
*.color3:       #f0c674
*.color11:      #f0c674
! blue
*.color4:       #81a2be
*.color12:      #81a2be
! magenta
*.color5:       #b294bb
*.color13:      #b294bb
! cyan
*.color6:       #8abeb7
*.color14:      #8abeb7
! white
*.color7:       #c5c8c6
*.color15:      #ffffff

! XFT
Xft.rgba: none
Xft.hintstyle: hintfull

! Emacs
Emacs.font: Ubuntu Mono-12:pixelsize=16
Emacs*menuBar: off
Emacs*toolBar: off
Emacs*verticalScrollBars: off

! Rxvt-unicode
URxvt*background: black
URxvt*foreground: white
URxvt*cursorColor: magenta
URxvt*font: xft:Ubuntu Mono:size=12:style=Regular
URxvt*boldFont: xft:Ubuntu Mono:size=12:style=Bold
URxvt*italicFont: xft:Ubuntu Mono:size=12:style=Italic
URxvt*boldItalicFont: xft:Ubuntu Mono:size=12:style=Bold Italic
URxvt*scrollBar: off
URxvt*transparent: on
URxvt*tintColor: white
URxvt*shading: 25
URxvt*blurRadius: 7x7

